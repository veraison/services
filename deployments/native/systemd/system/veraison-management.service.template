[Unit]
Description=Veraison management API server
After=veraison-trusted.service

[Service]
Type=exec
ExecStart=${VERAISON_BIN_DIR}/management-service --config ${VERAISON_CONFIG_DIR}/services/config.yaml
Restart=always
User=${VERAISON_USER}

[Install]
WantedBy=default.target
Requires=veraison-trusted.service

